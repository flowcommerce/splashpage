GET     /_internal_/healthcheck                 controllers.HealthchecksController.index()
GET     /                                       controllers.ApplicationController.index(video: Option[Int])
GET     /videos                                 controllers.ApplicationController.videos()

POST    /publications/:publication/subscribe    controllers.PublicationsController.postSubscribe(publication: io.flow.splashpage.v0.models.Publication)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file              controllers.WebJarAssets.at(file)